            </HBox>
            <HBox prefHeight="172.0" prefWidth="300.0" HBox.hgrow="ALWAYS">
               <children>
                  <VBox alignment="CENTER_LEFT" HBox.hgrow="ALWAYS">
                     <children>
                        <ImageView fx:id="profileImage" fitHeight="172.0" fitWidth="174.0" pickOnBounds="true" preserveRatio="true">
