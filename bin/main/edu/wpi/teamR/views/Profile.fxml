                  </HBox>
                  <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Text fx:id="phoneNumber" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="bodyMedium" text="(123)-456-7890" />
                     </children>
                     <padding>
                        <Insets right="20.0" />
                     </padding>
                  </HBox>
               </children>
            </HBox>
         </children>
         <VBox.margin>
