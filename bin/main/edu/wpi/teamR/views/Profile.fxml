                     </padding>
                  </HBox>
                  <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Text fx:id="name" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="bodyMedium" text="Shaggy Rogers" />
