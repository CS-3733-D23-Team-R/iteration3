<VBox prefHeight="735.0" prefWidth="488.0" styleClass="boxroundedbordered" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.teamR.controllers.ProfileController">
